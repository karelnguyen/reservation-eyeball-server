{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Reservation": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "scheduledAt": {
          "type": "string",
          "format": "date-time"
        },
        "pinHash": {
          "type": "string"
        },
        "pinSalt": {
          "type": "string"
        },
        "pinLast4": {
          "type": "string"
        },
        "status": {
          "type": "string",
          "default": "booked"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "confirmedAt": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "checkedInAt": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "actualEnd": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        }
      }
    }
  },
  "type": "object",
  "properties": {
    "reservation": {
      "$ref": "#/definitions/Reservation"
    }
  }
}